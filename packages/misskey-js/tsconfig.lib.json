{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "../shared/tsconfig.web.json",
	"compilerOptions": {
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true,
		"inlineSources": true,
		"outDir": "./temp/built/",
		"rootDir": ".",
		"exactOptionalPropertyTypes": true,
		"composite": true,
		"typeRoots": [
			"./node_modules/@types"
		]
	},
	"include": [
		"src/**/*",
		"test/**/*",
		"test-d/**/*"
	],
	"exclude": [
		"node_modules"
	]
}
