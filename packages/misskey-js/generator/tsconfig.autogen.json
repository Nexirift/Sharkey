{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "../../shared/tsconfig.web.json",
	"compilerOptions": {
		"noImplicitAny": false,
		"isolatedModules": false,
		"rootDirs": [
			"built",
			"../built"
		]
	},
	"include": [
		"built/autogen/**/*.ts"
	],
	"exclude": [
		"node_modules",
		"./built/**/*.js"
	]
}
