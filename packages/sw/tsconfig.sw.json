{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "../shared/tsconfig.webworker.json",
	"compilerOptions": {
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true,
		"inlineSources": true,
		"removeComments": false,
		"baseUrl": ".",
		"outDir": "../../built/_sw_dist_",
		"paths": {
			"@/*": ["./src/*"],
			"@@/*": ["../frontend-shared/*"]
		},
		"typeRoots": [
			"./node_modules/@types",
			"./src/@types"
		]
	},
	"include": [
		"src/**/*.ts"
	],
	"exclude": [
		"node_modules"
	]
}
